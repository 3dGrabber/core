<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <FSharpImport>false</FSharpImport>
    <FSharpTargets1>$(MSBuildExtensionsPath32)\..\Microsoft SDKs\F#\3.0\Framework\v4.0\Microsoft.FSharp.Targets</FSharpTargets1>
    <FSharpTargets2>$(MSBuildExtensionsPath32)\..\Microsoft F#\v4.0\Microsoft.FSharp.Targets</FSharpTargets2>
    <FSharpTargets3>$(MSBuildExtensionsPath32)\FSharp\1.0\Microsoft.FSharp.Targets</FSharpTargets3>
  </PropertyGroup>
  <Import Project="$(FSharpTargets1)" Condition="!$(FSharpImport) AND Exists('$(FSharpTargets1)')" />
  <PropertyGroup Condition="!$(FSharpImport) AND Exists('$(FSharpTargets1)')">
    <FSharpImport>true</FSharpImport>
  </PropertyGroup>
  <Import Project="$(FSharpTargets2)" Condition="!$(FSharpImport) AND Exists('$(FSharpTargets2)')" />
  <PropertyGroup Condition="!$(FSharpImport) AND Exists('$(FSharpTargets2)')">
    <FSharpImport>true</FSharpImport>
  </PropertyGroup>
  <Import Project="$(FSharpTargets3)" Condition="!$(FSharpImport) AND Exists('$(FSharpTargets3)')" />
  <PropertyGroup Condition="!$(FSharpImport) AND Exists('$(FSharpTargets3)')">
    <FSharpImport>true</FSharpImport>
  </PropertyGroup>
  <Import Project="$(MSBuildExtensionsPath32)\Microsoft\MicrosoftAjax\AjaxMin.tasks" />
  <Import Project="$(MSBuildExtensionsPath32)\ExtensionPack\4.0\MSBuild.ExtensionPack.tasks" />
  <PropertyGroup>
    <KeyOriginatorFile>$(MSBuildThisFileDirectory)\WebSharper.snk</KeyOriginatorFile>
    <AssemblySearchPaths>
      $(MSBuildThisFileDirectory)\vs11\packages\Mono.Cecil.0.9.5.2\lib\net35\.;
      $(AssemblySearchPaths)
    </AssemblySearchPaths>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(MSBuildThisFileDirectory)\AssemblyInfo.fs" />
  </ItemGroup>
</Project>
