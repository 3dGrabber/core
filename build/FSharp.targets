<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <Root>$(MSBuildThisFileDirectory)/..</Root>
    <Key1>$(INTELLIFACTORY)/keys/IntelliFactory.snk</Key1>
    <Key2>$(Root)/Build/WebSharper.snk</Key2>
    <KeyOriginatorFile Condition=" '$(KeyOriginatorFile)' == '' AND Exists('$(Key1)')">$(Key1)</KeyOriginatorFile>
    <KeyOriginatorFile Condition=" '$(KeyOriginatorFile)' == '' AND Exists('$(Key2)')">$(Key2)</KeyOriginatorFile>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="mscorlib" />
    <Reference Include="System" />
  </ItemGroup>
  <Import Condition=" '$(WebSharperProject)' != 'Site' " Project="$(Root)/.build/FSharp.targets" />
</Project>
